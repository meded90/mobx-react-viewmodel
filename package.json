{
  "name": "mobx-react-viewmodel",
  "version": "0.2.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint src test stories --ext .ts,.tsx",
    "lint-staged": "lint-staged",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "build-storybook": "NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "docs": "NODE_OPTIONS=--openssl-legacy-provider start-storybook --docs --no-manager-cache",
    "build-docs": "NODE_OPTIONS=--openssl-legacy-provider build-storybook --docs"
  },
  "peerDependencies": {
    "mobx": ">=6",
    "mobx-react": ">=7",
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts*": [
      "prettier --config .prettierrc --write",
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Iverson/mobx-react-viewmodel"
  },
  "author": "Alexey Krasman",
  "module": "dist/mobx-react-viewmodel.esm.js",
  "size-limit": [
    {
      "path": "dist/mobx-react-viewmodel.cjs.production.min.js",
      "limit": "4 KB"
    },
    {
      "path": "dist/mobx-react-viewmodel.esm.js",
      "limit": "1 KB"
    }
  ],
  "devDependencies": {
    "@a110/storybook-expand-all": "^1.1.1",
    "@babel/core": "^7.21.0",
    "@size-limit/esbuild": "^8.2.4",
    "@size-limit/esbuild-why": "^8.2.4",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-storysource": "^6.5.16",
    "@storybook/addons": "^6.5.16",
    "@storybook/react": "6.5.10",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "babel-loader": "^9.1.2",
    "eslint": "^9.39.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "mobx": "^6.8.0",
    "mobx-react": "^7.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "mobx",
    "react",
    "reactive",
    "mobservable",
    "observable",
    "react-component",
    "reactjs",
    "viewmodel"
  ]
}
